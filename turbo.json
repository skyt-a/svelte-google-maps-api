{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".svelte-kit/**", "build/**"]
		},
		"lint": {},
		"check": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"format": {},
		"test": {
			"dependsOn": ["^build"],
			"outputs": []
		}
	}
}
